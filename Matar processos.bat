@echo off

rem Significado do comando
rem - task = tarefa
rem - list = lista
rem - kill = matar

rem parametro do comando
rem /f = forçar
rem /t = fechar processos filhos
rem /im = matar o processo

rem "-" traço pode ser utilizado no lugar do "/"

taskkill /f /t /im Discord.exe

taskkill /f /t /im AnyDesk.exe

taskkill /f /t /im E_S50RPB.EXE

taskkill /f /t /im E_S50STB.EXE

exit